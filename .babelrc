{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "11",
          "node": "current"
        },
      }
    ],
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "h",
      "pragmaFlag": "Fragment"
    }],
    ["styled-jsx/babel"]
  ]
}
